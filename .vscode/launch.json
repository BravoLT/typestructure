  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/src/proxy.server.ts",
      "cwd": "${workspaceRoot}",
      "outFiles": ["${workspaceRoot}/bin/**/*.js"],
      "args":[
        "--debug-brk","53629",
        "--expose_debug_as", "v8debug"
      ],
      "sourceMaps": true,
      "preLaunchTask": "pre-debug-build"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858,
      "outFiles": ["${workspaceRoot}/bin/"],
      "sourceMaps": true
    }
  ]
}